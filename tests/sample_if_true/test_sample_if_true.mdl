oscilating var=
	SIN(EXP(Time))
	~	
	~		|

sample simple=SAMPLE IF TRUE(
	oscilating var<0,time in seconds, 100)
	~	
	~		|

sample var=
	SAMPLE IF TRUE(oscilating var>0, time in seconds, time in seconds)
	~	
	~		|

time in seconds=
	Time*60
	~	
	~		|

